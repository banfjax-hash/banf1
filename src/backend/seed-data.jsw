/**
 * Seed ALL collections with sample data.
 * Skips collections that already contain data (idempotent).
 *
 * @returns {Promise<Object>} Summary of seeded/skipped collections
 */
export async function seedAllCollections() {
    const collectionNames = Object.keys(SEED_DATA);
    const results = { seeded: [], skipped: [], failed: [], total: collectionNames.length };

    for (const name of collectionNames) {
        try {
            const result = await seedCollection(name);
            if (result.action === 'seeded') {
                results.seeded.push({ name, count: result.count });
            } else {
                results.skipped.push({ name, reason: result.reason });
            }
        } catch (err) {
            results.failed.push({ name, error: err.message || String(err) });
        }
    }

    results.summary = `Seeded: ${results.seeded.length}, Skipped: ${results.skipped.length}, Failed: ${results.failed.length}`;
    return results;
}

/**
 * Seed a single collection with sample data.
 *
 * @param {string} collectionName - Name of the Wix Data collection
 * @returns {Promise<Object>} Result with action and count
 */
export async function seedCollection(collectionName) {
    const records = SEED_DATA[collectionName];
    if (!records || records.length === 0) {
        return { collection: collectionName, action: 'skipped', reason: 'No seed data defined' };
    }

    // Check if collection already has data
    try {
        const existing = await wixData.query(collectionName)
            .limit(1)
            .find({ suppressAuth: true });

        if (existing.items.length > 0) {
            return { collection: collectionName, action: 'skipped', reason: 'Collection already has data', existingCount: existing.totalCount };
        }
    } catch (_) {
        // Collection doesn't exist yet ‚Äî will be created on first insert
    }

    // Insert all records
    let insertedCount = 0;
    for (const record of records) {
        try {
            await wixData.insert(collectionName, record, { suppressAuth: true });
            insertedCount++;
        } catch (err) {
            console.error(`[seed-data] Failed to insert into ${collectionName}:`, err);
        }
    }

    return { collection: collectionName, action: 'seeded', count: insertedCount, total: records.length };
}

/**
 * Clear all data from a single collection.
 * ‚ö†Ô∏è  Use with caution ‚Äî deletes ALL records!
 *
 * @param {string} collectionName - Name of the collection to clear
 * @returns {Promise<Object>} Result with count of deleted records
 */
export async function clearCollection(collectionName) {
    let deletedCount = 0;
    let hasMore = true;

    while (hasMore) {
        const results = await wixData.query(collectionName)
            .limit(100)
            .find({ suppressAuth: true });

        if (results.items.length === 0) {
            hasMore = false;
            break;
        }

        for (const item of results.items) {
            try {
                await wixData.remove(collectionName, item._id, { suppressAuth: true });
                deletedCount++;
            } catch (_) { /* skip */ }
        }

        if (results.items.length < 100) {
            hasMore = false;
        }
    }

    return { collection: collectionName, deleted: deletedCount };
}

/**
 * Clear ALL collections and re-seed with fresh sample data.
 * ‚ö†Ô∏è  DESTRUCTIVE ‚Äî removes all existing data first!
 *
 * @returns {Promise<Object>} Combined clear + seed results
 */
export async function resetAllCollections() {
    const collectionNames = Object.keys(SEED_DATA);
    const clearResults = [];

    // Step 1: Clear all
    for (const name of collectionNames) {
        try {
            const result = await clearCollection(name);
            clearResults.push(result);
        } catch (_) {
            clearResults.push({ collection: name, deleted: 0, error: 'clear failed' });
        }
    }

    // Step 2: Seed all
    const seedResults = await seedAllCollections();

    return {
        cleared: clearResults,
        seeded: seedResults,
        summary: `Cleared ${clearResults.reduce((sum, r) => sum + r.deleted, 0)} records, then ${seedResults.summary}`
    };
}

/**
 * Get a list of all collections that have seed data defined.
 *
 * @returns {Object} List of collection names and record counts
 */
export function getAvailableSeedCollections() {
    const collections = {};
    for (const [name, records] of Object.entries(SEED_DATA)) {
        collections[name] = records.length;
    }
    return {
        collections,
        totalCollections: Object.keys(collections).length,
        totalRecords: Object.values(collections).reduce((sum, n) => sum + n, 0)
    };
}
